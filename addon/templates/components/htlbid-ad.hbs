{{#if (has-block)}}
  {{yield (hash
    tag=(component "htlbid-ad/tag"
         slot=slotName
         isEager=@isEager
         isOOP=@isOOP
         sizes=@sizes
         ref=ref
         slotClassNames=slotClassNames
        )
    isEmpty=this.isEmpty
    width=this.width
    height=this.height
  )}}
{{else}}
  {{component "htlbid-ad/tag"
    slot=slotName
    isEager=@isEager
    isOOP=@isOOP
    sizes=@sizes
    ref=ref
    slotClassNames=slotClassNames
  }}
{{/if}}